{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
        ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/public/(.*)",
            "dest": "/public/"
        }
    ],
    "env": {
        "MY_VAR": "my-value"
    }
}
